<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true" >
        <shape>
            <gradient
                android:startColor="#88C6F9FF"
                android:endColor="#88C6F9FF"
                />
            <stroke
                android:width="10dp"
                android:color="#88C6F9FF" />
            <corners
                android:radius="10dp" />
           
        </shape>
    </item>

</selector>